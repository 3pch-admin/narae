	<bean class="ext.narae.mvc.change.NaraeECRTableBuilder"></bean>
	<bean class="ext.narae.mvc.document.NaraeDocumentTableBuilder"></bean>
	<bean class="ext.narae.mvc.part.NaraePartTableBuilder"></bean>
</beans>	